<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.parakh.colpedia" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>


        <activity
            android:name=".academic"
            android:label="@string/title_activity_academic" >
            <intent-filter>
                <action android:name="com.example.parakh.colpedia.academic" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>


        <activity
            android:name=".security"
            android:label="@string/title_activity_security" >
            <intent-filter>
                <action android:name="com.example.parakh.colpedia.security" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>


        <activity
            android:name=".stationary"
            android:label="@string/title_activity_stationary" >
            <intent-filter>
                <action android:name="com.example.parakh.colpedia.stationary" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>


        <activity
            android:name=".fooding"
            android:label="@string/title_activity_fooding" >
            <intent-filter>
                <action android:name="com.example.parakh.colpedia.fooding" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>

        <activity
            android:name=".ragging"
            android:label="@string/title_activity_ragging" >

            <intent-filter>
                <action android:name="com.example.parakh.colpedia.ragging" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
    </application>

</manifest>
